
{%
  set classes = [
  'component',
  'component--type-' ~ paragraph.bundle|clean_class,
  'component-paragraph',
  'container',
  'page-width',
  'padded-row'
]
%}

<div{{ attributes.addClass(classes) }} data-aos="fade">
  <div class="component-wrapper">
    <div class="component-content">

      <div class="printable-wrapper">
        <a href="/media/{{ paragraph.field_printable_calendar.target_id }}/download/?inline" class="button">Print Calendar</a>
      </div>

      <div class="year-wrapper">
        <h2>{{ content.field_calendar_year.0 }}</h2>
      </div>

    </div>
  </div>
</div>
